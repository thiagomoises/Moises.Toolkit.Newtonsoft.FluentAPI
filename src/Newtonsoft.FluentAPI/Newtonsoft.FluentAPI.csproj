<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>Moises.Toolkit.Newtonsoft.FluentAPI</PackageId>
    <Authors>T. Moises</Authors>
    <Product>Moises.Toolkit.Newtonsoft.FluentAPI</Product>
    <Description>Fluent, XML-less, compile safe, automated, convention-based mappings for Json.NET.</Description>
    <PackageProjectUrl>https://github.com/thiagomoises/moisesToolkit.Newtonsoft.FluentAPI</PackageProjectUrl>
    <RepositoryUrl>https://github.com/thiagomoises/moisesToolkit.Newtonsoft.FluentAPI</RepositoryUrl>
    <RepositoryType>public</RepositoryType>
    <PackageTags>json, newtonsoft, newtonsoft-json, fluent-api, fluent-api-json</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>2.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

</Project>
